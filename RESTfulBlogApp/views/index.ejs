<% include ./partials/header %>
<h1>This is index</h1>

<% 
    blogs.forEach((blog)=>{ %>
        <div>
            <h2><%=  blog.title %></h2>
            <img src="<%=blog.image %>" alt="<%= blog.title %>">
            <span><%= blog.created.toDateString() %></span>
            <p><%- blog.body.substring(0,100) %>...</p>
            <a href="/blogs/<%= blog.id %>">Read More</a>
    </div>
    <% }); %>
    <% include ./partials/footer %>